  <groupId>com.gene.coin</groupId>
  <artifactId>gen</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>gen</name>
  <url>http://maven.apache.org</url>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies>
